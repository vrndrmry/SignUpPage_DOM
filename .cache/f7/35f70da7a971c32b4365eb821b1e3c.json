Id(id).parentElement.classList.remove(\"error\");\r\n    }else{\r\n        document.getElementById(id).parentElement.classList.add(\"error\")\r\n    }\r\n\r\n}\r\n\r\nformEl.addEventListener(\"submit\", function (evt) {\r\n  evt.preventDefault();\r\n  const formElements = Array.from(formEl).filter(\r\n    (ele) => ele.id != \"submitBtn\"\r\n  );\r\n//   let dataArr = [];\r\n//   formElements.forEach((elem) => { [\r\n//       ...dataArr,\r\n//       dataArr.push({\r\n//         id: elem.id,\r\n//         type: elem.type,\r\n//         value: elem.value.trimLeft(),\r\n//       }),\r\n//     ];\r\n    \r\n//   });\r\n  console.log(formElements)\r\nformElements.forEach(elem => elem.type != \"checkbox\" ? validator(elem.id,elem.type,elem.value) : validator(elem.id,elem.type,elem.checked))\r\n});\r\n"},"lineCount":null}},"error":null,"hash":"fa032a0e616bdbca8e65bce2385c21be","cacheData":{"env":{}}}